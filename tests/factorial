main()

def fact( n )

   if le(n, 1) 
      return 1
   else
      temp = fact( n-1 )
      "n * temp" nl()
      "n is " print(n) nl()
      "temp is " print(temp) nl()
      return n * temp
   end

end

def main()

  "enter n: "
  n = input()
  print(n)   "! = "   print(fact(n))    nl()

return 0
end
